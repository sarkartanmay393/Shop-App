pipeline {
    agent any
    stages {
        stage("build") {
            echo "Building stage is running."
        }
        stage("test") {
            echo "test stage is running."
            script {
                def aLamdaFunction = 3 + 0 < 4 ? 'right"' : 'left'
                echo aLamdaFunction
            }
        }
        stage("deploy") {
            echo "Building stage is running."
        }
    }
}